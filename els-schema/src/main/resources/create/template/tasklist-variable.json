{
	"order": 30,
	"index_patterns": [
		"tasklist-variable-${schema.version}_*"
	],
	"mappings": {
		"dynamic": "strict",
		"dynamic_templates": [
			{
				"string_template": {
					"path_match": "*",
					"mapping": {
						"norms": {
							"enabled": false
						},
						"type": "string",
						"index_options": "docs"
					},
					"match_mapping_type": "string"
				}
			}
		],
		"properties": {
			"partitionId": {
				"type": "integer"
			},
			"name": {
				"type": "keyword"
			},
			"scopeKey": {
				"type": "long"
			},
			"workflowInstanceKey": {
				"type": "long"
			},
			"id": {
				"type": "keyword"
			},
			"value": {
				"type": "keyword",
				"ignore_above": 8191
			},
			"key": {
				"type": "long"
			}
		}
	},
	"aliases": {
		"tasklist-variable-${schema.version}_alias": {
		}
	}
}
