					"match_mapping_type": "string",
					"mapping": {
						"index_options": "docs",
						"norms": {
							"enabled": false
						},
						"type": "string"
					}
				}
			}
		],
		"properties": {
			"id": {
				"type": "keyword"
			},
			"password": {
				"type": "keyword"
			},
			"role": {
				"type": "keyword"
			},
			"username": {
				"type": "keyword"
			},
			"firstname": {
				"type": "keyword"
			},
			"lastname": {
				"type": "keyword"
			}
		}
	}
}
