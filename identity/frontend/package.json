{
  "name": "identity",
  "version": "8.6.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build && cp -r src/locales build/",
    "postbuild": "yarn build:license",
    "build:license": "cp ../license/License.txt build/License.txt",
    "document:prepare": "rm -rf node_modules && yarn install --frozen-lockfile",
    "document:generate": "license-checker --out docs/dependencies.json --production --json --relativeLicensePath",
    "document:format": "node docs/formatDependencies.js",
    "document:cleanup": "rm docs/dependencies.json",
    "document": "yarn document:prepare && yarn document:generate && yarn document:format && yarn document:cleanup",
    "format": "prettier --write .",
    "start": "PORT=14000 react-app-rewired start",
    "test:format": "prettier --check .",
    "test:licenses": "license-checker --excludePrivatePackages --excludePackages 'wicg-inert@3.1.1' --onlyAllow 'Apache-2.0;MIT;0BSD;BSD-2-Clause;BSD-3-Clause;ISC;CC0-1.0;WTFPL;OFL-1.1' --production",
    "test:lint": "eslint --ext .ts --ext .tsx src/",
    "test:unit": "env-cmd -f .env -f .env.test react-scripts test --watchAll=false",
    "test": "yarn test:format && yarn test:licenses && yarn test:lint && yarn test:unit"
  },
  "engines": {
    "node": "18.19.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "~(.*)": "<rootDir>/src/$1",
      "react-markdown": "<rootDir>/node_modules/react-markdown/react-markdown.min.js"
    },
    "transform": {},
    "transformIgnorePatterns": [
      "/node_modules/(?!(@ibm|axios|@camunda))"
    ]
  },
  "dependencies": {
    "@camunda-cloud/c4-identity": "0.0.22",
    "@camunda/camunda-composite-components": "^0.6.0",
    "@carbon/layout": "11.20.1",
    "@carbon/motion": "11.16.1",
    "@carbon/react": "1.42.1",
    "@carbon/themes": "11.32.0",
    "@ibm/plex": "6.4.0",
    "axios": "1.6.7",
    "copy-to-clipboard": "3.3.3",
    "event-source-polyfill": "1.0.31",
    "i18next": "22.5.1",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-xhr-backend": "3.2.2",
    "react": "18.2.0",
    "react-debounced": "1.1.2",
    "react-dom": "18.2.0",
    "react-i18next": "12.3.1",
    "react-is": "18.2.0",
    "react-router": "6.22.3",
    "react-router-dom": "6.22.3",
    "react-transition-group": "4.4.5",
    "styled-components": "^6.0.0",
    "stylis": "^4.0.0",
    "universal-cookie": "4.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/preset-env": "7.24.3",
    "@remix-run/router": "1.15.3",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/carbon__layout": "0.0.3",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.26",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/react-router-dom": "5.3.3",
    "@types/react-transition-group": "4.4.10",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "^29.7.0",
    "body-parser": "1.20.2",
    "env-cmd": "10.1.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "express-http-proxy": "1.6.3",
    "license-checker": "25.0.1",
    "msw": "1.3.3",
    "prettier": "2.8.8",
    "react-app-rewired": "2.2.1",
    "react-scripts": "5.0.1",
    "sass": "1.71.1",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.4.3"
  },
  "packageManager": "yarn@3.8.1",
  "resolutions": {
    "@babel/traverse": "7.24.1",
    "jest-snapshot/@babel/traverse": "7.24.1"
  }
}
