{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@bpmn-io/dmn-migrate": "^0.4.3",
    "@bpmn-io/element-templates-icons-renderer": "^0.3.0",
    "@camunda/camunda-composite-components": "^0.0.40",
    "@carbon/icons-react": "^11.20.0",
    "@carbon/react": "^1.30.0",
    "@lexical/react": "^0.7.6",
    "@ungap/structured-clone": "^1.2.0",
    "bpmn-js": "13.1.0",
    "bpmn-js-disable-collapsed-subprocess": "^0.1.7",
    "chart.js": "^4.3.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "debounce": "^1.2.1",
    "dmn-js": "^14.1.5",
    "fast-deep-equal": "^3.1.3",
    "fitty": "^2.3.7",
    "focus-visible": "^5.2.0",
    "heatmap.js": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "inferno": "~5.6.0",
    "lexical": "^0.7.6",
    "react": "17.0.2",
    "react-date-range": "^1.4.0",
    "react-dom": "17.0.2",
    "react-full-screen": "^1.1.1",
    "react-grid-layout": "^1.3.4",
    "react-markdown": "^8.0.7",
    "react-resizable": "^3.0.5",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.3.0",
    "react-table": "^7.8.0",
    "table-js": "^8.0.2",
    "use-deep-compare-effect": "^1.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "generate-data": "node scripts/generate-data",
    "test": "react-scripts test --verbose=false",
    "test:ci": "CI=true react-scripts --expose-gc --max_old_space_size=1024 test --maxWorkers=$LIMITS_CPU --ci --testResultsProcessor='jest-junit'",
    "screenshots": "testcafe chrome e2e/tests/*.js -s thumbnails=false,path=../../camunda-platform-docs/docs/components/optimize/userguide ",
    "e2e": "testcafe -c 3 chrome e2e/tests/*.js --disable-screenshots",
    "e2e:local": "node scripts/e2e.mjs",
    "e2e:ci-browserstack": "node scripts/e2e.mjs ci browserstack",
    "e2e:ci:headless": "testcafe -c 3 chrome:headless e2e/tests/*.js -e --disable-screenshots --assertion-timeout 40000",
    "e2e:ci:cloud:headless": "testcafe chrome:headless e2e/cloud-tests/smokeTest.js -e",
    "e2e:cloud": "testcafe chrome e2e/cloud-tests/smokeTest.js",
    "eject": "react-scripts eject",
    "postinstall": "node scripts/wireModules",
    "start-backend": "node scripts/start-backend",
    "start-backend-cloud": "node scripts/start-backend cloud",
    "start-backend-sm": "node scripts/start-backend self-managed",
    "restart-backend": "curl http://localhost:8100/api/restartBackend",
    "dependencies": "node scripts/dependencies.js",
    "prepare": "cd .. && husky install client/.husky"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest-junit": {
    "suiteName": "client",
    "outputDirectory": ".",
    "outputName": "jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "@types/carbon__icons-react": "^11.20.0",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "29.5.2",
    "@types/node": "^20.2.3",
    "@types/react": "^18.0.11",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.14",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "ansi-html": "^0.0.9",
    "chalk": "^4.1.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.1.3",
    "enzyme": "3.11.0",
    "eslint-plugin-camunda-licensed": "^0.4.6",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "event-source-polyfill": "^1.0.31",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^16.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.2.2",
    "mock-socket": "^9.2.1",
    "node-fetch": "^2.6.7",
    "old-bpmn-js": "npm:bpmn-js@8.10",
    "opn": "^6.0.0",
    "prettier": "2.8.8",
    "react-scripts": "5.0.0",
    "sass": "^1.62.1",
    "shelljs": "^0.8.5",
    "styled-components": "^5.3.11",
    "testcafe": "^2.6.2",
    "testcafe-browser-provider-browserstack": "^1.14.2",
    "tree-kill": "^1.2.2",
    "typescript": "^5.1.3",
    "ws": "^8.13.0",
    "xml2js": "^0.6.0"
  },
  "resolutions": {
    "react-scripts/**/dot-prop": "^5.1.1",
    "**/jpeg-js": "^0.4.0",
    "**/kind-of": "^6.0.3"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "/node_modules/(?!@camunda/camunda-composite-components)"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
