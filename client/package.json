{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@bpmn-io/dmn-migrate": "^0.4.3",
    "@ibm/plex": "^5.2.1",
    "bpmn-js": "8.7.3",
    "bpmn-js-disable-collapsed-subprocess": "^0.1.3",
    "chart.js": "^3.5.1",
    "chartjs-plugin-datalabels": "^2.0.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "debounce": "^1.2.1",
    "dmn-js": "^11.0.2",
    "fast-deep-equal": "^3.1.3",
    "fitty": "^2.3.5",
    "focus-visible": "^5.2.0",
    "heatmap.js": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "inferno": "~5.6.0",
    "react": "17.0.2",
    "react-date-range": "^1.4.0",
    "react-dom": "17.0.2",
    "react-full-screen": "^1.1.0",
    "react-grid-layout": "^1.3.0",
    "react-markdown": "^7.0.1",
    "react-resizable": "^3.0.4",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.3.0",
    "react-table": "^7.7.0",
    "table-js": "^7.2.0",
    "use-deep-compare-effect": "^1.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node scripts/license-build.js",
    "generate-data": "node scripts/generate-data",
    "test": "react-scripts test --verbose=false",
    "test:ci": "CI=true react-scripts test --maxWorkers=$LIMITS_CPU --ci --testResultsProcessor='jest-junit'",
    "screenshots": "testcafe chrome e2e/tests/*.js -s ../../camunda-docs-optimize/content/user-guide",
    "e2e": "testcafe -c 3 chrome e2e/tests/*.js --disable-screenshots",
    "e2e:local": "node scripts/e2e",
    "e2e:ci-browserstack": "node scripts/e2e ci browserstack",
    "e2e:ci-chromeheadless": "node scripts/e2e ci chromeheadless",
    "eject": "react-scripts eject",
    "postinstall": "node scripts/wireModules",
    "start-backend": "node scripts/start-backend",
    "start-backend-withZeebe": "node scripts/start-backend zeebe",
    "restart-backend": "curl http://localhost:8100/api/restartBackend",
    "dependencies": "node scripts/dependencies.js",
    "prepare": "cd .. && husky install client/.husky"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest-junit": {
    "suiteName": "client",
    "outputDirectory": ".",
    "outputName": "jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "ansi-html": "^0.0.7",
    "chalk": "^4.1.2",
    "date-fns": "^2.23.0",
    "enzyme": "3.11.0",
    "eslint-plugin-camunda-licensed": "^0.4.4",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "glob": "^7.1.7",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.2",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^12.2.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^11.1.2",
    "mock-socket": "^9.0.3",
    "node-fetch": "^2.6.1",
    "opn": "^6.0.0",
    "prettier": "2.4.0",
    "react-scripts": "4.0.3",
    "sass": "^1.42.1",
    "shelljs": "^0.8.4",
    "testcafe": "^1.16.0",
    "testcafe-browser-provider-browserstack": "^1.13.1",
    "tree-kill": "^1.2.2",
    "ws": "^8.2.2",
    "xml2js": "^0.4.23"
  },
  "resolutions": {
    "react-scripts/**/dot-prop": "^5.1.1",
    "**/jpeg-js": "^0.4.0",
    "**/kind-of": "^6.0.3"
  },
  "jest": {
    "resetMocks": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
