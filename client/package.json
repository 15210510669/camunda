    "glob": "^7.1.5",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.0.9",
    "jest-enzyme": "^7.1.1",
    "jest-junit": "^8.0.0",
    "js-yaml": "^3.13.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^9.2.5",
    "mock-socket": "^9.0.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "opn": "^6.0.0",
    "prettier": "1.18.2",
    "react-scripts": "3.2.0",
    "request": "^2.88.0",
    "shelljs": "^0.8.3",
    "tarball-extract": "^0.0.6",
    "testcafe": "^1.6.0",
    "testcafe-browser-provider-browserstack": "^1.8.0",
    "tree-kill": "^1.2.1",
    "ws": "^7.2.0",
    "xml2js": "^0.4.22"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
