  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node scripts/license-build.js",
    "test": "react-scripts test --verbose=false",
    "test:ci": "CI=true react-scripts test --maxWorkers=$LIMITS_CPU --ci --testResultsProcessor='jest-junit'",
    "screenshots": "testcafe chrome e2e/tests/*.js -s ../../camunda-docs-optimize/content/user-guide",
    "e2e": "testcafe chrome e2e/tests/*.js --disable-screenshots",
    "e2e:local": "node scripts/e2e",
    "e2e:ci-browserstack": "node scripts/e2e ci browserstack",
    "e2e:ci-chromeheadless": "node scripts/e2e ci chromeheadless",
