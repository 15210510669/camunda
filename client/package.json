{
  "name": "camunda-cloud-operate",
  "version": "1.1.0-SNAPSHOT",
  "private": true,
  "dependencies": {
    "@camunda-cloud/common-ui": "^0.0.17-rc2",
    "@camunda-cloud/common-ui-react": "^0.0.17-rc2",
    "@popperjs/core": "^2.9.3",
    "bpmn-js": "^8.7.3",
    "bpmn-moddle": "^7.1.2",
    "brace": "^0.11.1",
    "date-fns": "^2.23.0",
    "final-form": "^4.20.2",
    "jsoneditor": "^9.5.5",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "msw-storybook-addon": "^1.2.0",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-contenteditable": "^3.3.6",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
    "react-popper": "^2.2.5",
    "react-router-dom": "^5.2.1",
    "react-textarea-autosize": "^8.3.3",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "start": "TSC_COMPILE_ON_ERROR=true react-scripts start",
    "start:e2e": "BROWSER=none PORT=3001 IS_E2E=true react-scripts start",
    "build": "react-scripts build && node license-build.js",
    "test": "react-scripts test --env=jsdom --resetMocks=false",
    "test:ci": "CI=true react-scripts test --env=jsdom --resetMocks=false --maxWorkers=$LIMITS_CPU --ci --reporters=default --reporters=jest-junit --testTimeout 7000",
    "eject": "react-scripts eject",
    "lint": "tsc && eslint src/ e2e/",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "test:e2e": "ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 testcafe chrome e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:e2e:ci": "PORT=8080 testcafe chrome:headless e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:browserstack": "ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 node e2e/scripts/browserstack.js",
    "test:ci-browserstack": "PORT=8080 node e2e/scripts/browserstack.js",
    "postinstall": "cd .. && husky install",
    "coverage": "yarn test --coverage --collectCoverageFrom=src/**/* --changedSince=origin/master --watchAll=false --passWithNoTests",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest-junit": {
    "suiteName": "client",
    "outputName": "jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/testcafe": "^4.4.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/history": "4.7.9",
    "@types/jest": "27.0.1",
    "@types/jsoneditor": "8.6.1",
    "@types/lodash": "4.14.172",
    "@types/node": "16.7.10",
    "@types/node-fetch": "2.5.12",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/react-transition-group": "4.4.2",
    "@types/styled-components": "5.1.13",
    "babel-loader": "8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-camunda-licensed": "^0.4.4",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.7",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.2",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^12.2.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "msw": "^0.35.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "testcafe": "^1.15.3",
    "testcafe-browser-provider-browserstack": "^1.13.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.4.2",
    "zeebe-node": "1.3.4"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scarfSettings": {
    "enabled": false
  },
  "msw": {
    "workerDirectory": "public"
  }
}