    "analyze-bundle": "yarn build && source-map-explorer 'build/assets/*.js'",
    "playwright": "playwright test",
    "start-visual-regression-docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.40.1 /bin/bash",
    "start:visual-regression": "serve build/ -p 8081 -n -s -L",
    "build:visual-regression": "VITE_VERSION=0.0.0-SNAPSHOT yarn build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@axe-core/playwright": "4.8.2",
