{
  "name": "camunda-operate",
  "version": "8.2.0-SNAPSHOT",
  "private": true,
  "dependencies": {
    "@bpmn-io/element-template-icon-renderer": "0.4.0",
    "@camunda-cloud/common-ui": "0.0.18-rc13",
    "@camunda-cloud/common-ui-react": "0.0.18-rc13",
    "@camunda/camunda-composite-components": "0.0.28",
    "@carbon/elements": "11.20.0",
    "@carbon/react": "1.27.0",
    "@devbookhq/splitter": "1.4.0",
    "@floating-ui/react-dom": "1.3.0",
    "@loadable/component": "^5.15.3",
    "@monaco-editor/react": "4.4.6",
    "@types/loadable__component": "^5.13.4",
    "bpmn-js": "10.3.0",
    "bpmn-moddle": "8.0.1",
    "date-fns": "2.29.3",
    "dmn-js": "13.0.0",
    "final-form": "4.20.9",
    "final-form-arrays": "3.1.0",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "mixpanel-browser": "2.45.0",
    "mobx": "6.8.0",
    "mobx-react": "7.6.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-router-dom": "6.4.2",
    "react-transition-group": "4.4.5",
    "sass": "1.60.0",
    "styled-components": "5.3.9"
  },
  "scripts": {
    "start": "cross-env TSC_COMPILE_ON_ERROR=true GENERATE_SOURCEMAP=false react-scripts start",
    "start:e2e": "cross-env GENERATE_SOURCEMAP=false BROWSER=none PORT=3001 IS_E2E=true react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build && node license-build.js",
    "test": "react-scripts test --env=jsdom --resetMocks=false --clearMocks=true",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --resetMocks=false --ci --testTimeout 7000 --clearMocks=true",
    "eject": "react-scripts eject",
    "ts-check": "tsc && tsc --noEmit --project e2e/tsconfig.json",
    "eslint": "eslint src/ e2e/ --max-warnings=0",
    "lint": "yarn ts-check && yarn eslint",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "test:e2e": "cross-env ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 testcafe chrome e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:e2e:ci": "cross-env PORT=8080 testcafe chrome:headless e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "postinstall": "cd .. && husky install",
    "coverage": "yarn test --coverage --collectCoverageFrom=src/**/* --changedSince=origin/master --watchAll=false --passWithNoTests",
    "analyze-bundle": "react-scripts build && source-map-explorer 'build/static/js/*.js'"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@camunda-cloud/common-ui.*|@camunda/camunda-composite-components)"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/testcafe": "5.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/carbon-components-react": "7.55.2",
    "@types/carbon__elements": "11.10.0",
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.191",
    "@types/mixpanel-browser": "2.38.1",
    "@types/node": "18.15.5",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-transition-group": "4.4.5",
    "@types/styled-components": "5.1.26",
    "cross-env": "^7.0.3",
    "eslint-plugin-camunda-licensed": "0.4.6",
    "eslint-plugin-prettier": "4.2.1",
    "glob": "8.1.0",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.3",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.2.0",
    "monaco-editor": "0.36.1",
    "msw": "0.42.1",
    "node-fetch": "2.6.9",
    "prettier": "2.8.6",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
    "source-map-explorer": "2.5.3",
    "testcafe": "2.5.0",
    "ts-toolbelt": "9.6.0",
    "typescript": "5.0.4",
    "zeebe-node": "8.1.2"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scarfSettings": {
    "enabled": false
  },
  "msw": {
    "workerDirectory": "public"
  },
  "resolutions": {
    "react-scripts/postcss-preset-env/postcss-custom-properties": "^13.0.0",
    "@testing-library/dom": "9.2.0"
  }
}
