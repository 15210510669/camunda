{
  "name": "camunda-cloud-operate",
  "version": "1.3.0-SNAPSHOT",
  "private": true,
  "dependencies": {
    "@camunda-cloud/common-ui": "0.0.18-rc3",
    "@camunda-cloud/common-ui-react": "0.0.18-rc3",
    "@popperjs/core": "2.11.2",
    "bpmn-js": "8.9.1",
    "bpmn-moddle": "7.1.2",
    "brace": "0.11.1",
    "date-fns": "2.28.0",
    "final-form": "4.20.6",
    "jsoneditor": "9.7.0",
    "lodash": "4.17.21",
    "mixpanel-browser": "2.43.0",
    "mobx": "6.3.13",
    "mobx-react": "7.2.1",
    "msw-storybook-addon": "1.6.0",
    "polished": "4.1.4",
    "react": "17.0.2",
    "react-contenteditable": "3.3.6",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.8",
    "react-popper": "2.2.5",
    "react-router-dom": "5.3.0",
    "react-textarea-autosize": "8.3.3",
    "react-transition-group": "4.4.2",
    "styled-components": "5.3.3"
  },
  "scripts": {
    "start": "TSC_COMPILE_ON_ERROR=true react-scripts start",
    "start:e2e": "BROWSER=none PORT=3001 IS_E2E=true react-scripts start",
    "build": "react-scripts build && node license-build.js",
    "test": "react-scripts test --env=jsdom --resetMocks=false",
    "test:ci": "CI=true react-scripts test --env=jsdom --resetMocks=false --maxWorkers=$LIMITS_CPU --ci --reporters=default --reporters=jest-junit --testTimeout 7000",
    "eject": "react-scripts eject",
    "lint": "tsc && tsc --noEmit --project e2e/tsconfig.json && eslint src/ e2e/",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "test:e2e": "ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 testcafe chrome e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:e2e:ci": "PORT=8080 testcafe chrome:headless e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:browserstack": "ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 node e2e/scripts/browserstack.js",
    "test:ci-browserstack": "PORT=8080 node e2e/scripts/browserstack.js",
    "postinstall": "cd .. && husky install",
    "coverage": "yarn test --coverage --collectCoverageFrom=src/**/* --changedSince=origin/master --watchAll=false --passWithNoTests",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@camunda-cloud/common-ui.*)"
    ]
  },
  "jest-junit": {
    "suiteName": "client",
    "outputName": "jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.3.12",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.3.13",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/testcafe": "4.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/history": "4.7.11",
    "@types/jest": "27.4.0",
    "@types/jsoneditor": "9.5.1",
    "@types/lodash": "4.14.178",
    "@types/mixpanel-browser": "2.36.0",
    "@types/node": "16.11.22",
    "@types/node-fetch": "2.5.12",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/react-transition-group": "4.4.4",
    "@types/styled-components": "5.1.21",
    "babel-loader": "8.1.0",
    "eslint-plugin-camunda-licensed": "0.4.4",
    "eslint-plugin-prettier": "4.0.0",
    "glob": "7.2.0",
    "http-proxy-middleware": "2.0.2",
    "husky": "7.0.4",
    "jest-junit": "13.0.0",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.3.2",
    "msw": "0.36.8",
    "node-fetch": "2.6.7",
    "prettier": "2.5.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "testcafe": "1.18.3",
    "testcafe-browser-provider-browserstack": "1.13.1",
    "ts-toolbelt": "9.6.0",
    "typescript": "4.5.5",
    "zeebe-node": "2.4.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scarfSettings": {
    "enabled": false
  },
  "msw": {
    "workerDirectory": "public"
  }
}
