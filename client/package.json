  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node scripts/license-build.js",
    "test": "react-scripts test --verbose=false",
    "test:ci": "CI=true react-scripts test --maxWorkers=$LIMITS_CPU --ci --testResultsProcessor='jest-junit'",
    "screenshots": "testcafe chrome e2e/tests/*.js -s ../../camunda-docs-optimize/content/user-guide",
    "e2e": "testcafe -c 3 chrome e2e/tests/*.js --disable-screenshots",
    "e2e:local": "node scripts/e2e",
    "e2e:ci-browserstack": "node scripts/e2e ci browserstack",
    "e2e:ci-chromeheadless": "node scripts/e2e ci chromeheadless",
    "eject": "react-scripts eject",
    "postinstall": "node scripts/wireModules",
    "start-backend": "node scripts/start-backend",
    "restart-backend": "curl http://localhost:8100/api/restartBackend",
    "dependencies": "node scripts/dependencies.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest-junit": {
    "suiteName": "client",
    "output": "./jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "ansi-html": "^0.0.7",
    "chalk": "^4.0.0",
    "date-fns": "^2.14.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-plugin-camunda-licensed": "^0.4.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-testcafe": "^0.2.1",
    "glob": "^7.1.6",
    "http-proxy-middleware": "^1.0.3",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^10.0.0",
    "js-yaml": "^3.13.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.2.2",
    "mock-socket": "^9.0.3",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "opn": "^6.0.0",
    "prettier": "2.0.5",
    "react-scripts": "3.4.1",
    "shelljs": "^0.8.4",
    "tarball-extract": "^0.0.6",
    "testcafe": "^1.8.4",
    "testcafe-browser-provider-browserstack": "^1.12.0",
    "tree-kill": "^1.2.2",
    "ws": "^7.3.0",
    "xml2js": "^0.4.23"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
