    "start-backend": "node scripts/start-backend",
    "generate-data": "node scripts/generate-data",
    "dependencies": "node scripts/dependencies.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest-junit": {
    "suiteName": "client",
    "output": "./jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "ansi-html": "^0.0.7",
    "chalk": "^2.4.2",
