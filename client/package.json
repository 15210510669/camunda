{
  "name": "camunda-operate",
  "version": "8.1.0-SNAPSHOT",
  "private": true,
  "dependencies": {
    "@bpmn-io/element-templates-icons-renderer": "0.2.0",
    "@camunda-cloud/common-ui": "0.0.18-rc10",
    "@camunda-cloud/common-ui-react": "0.0.18-rc10",
    "@carbon/elements": "11.7.0",
    "@carbon/react": "1.9.0",
    "@devbookhq/splitter": "1.3.2",
    "@monaco-editor/react": "4.4.5",
    "@popperjs/core": "2.11.6",
    "bpmn-js": "9.3.2",
    "bpmn-moddle": "7.1.2",
    "date-fns": "2.29.1",
    "dmn-js": "12.2.1",
    "final-form": "4.20.7",
    "final-form-arrays": "3.0.2",
    "lodash": "4.17.21",
    "mixpanel-browser": "2.45.0",
    "mobx": "6.6.1",
    "mobx-react": "7.5.2",
    "polished": "4.2.2",
    "react": "17.0.2",
    "react-contenteditable": "3.3.6",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.3",
    "react-popper": "2.3.0",
    "react-router-dom": "6.3.0",
    "react-textarea-autosize": "8.3.4",
    "react-transition-group": "4.4.5",
    "sass": "1.54.4",
    "styled-components": "5.3.5"
  },
  "scripts": {
    "start": "TSC_COMPILE_ON_ERROR=true react-scripts start",
    "start:e2e": "BROWSER=none PORT=3001 IS_E2E=true react-scripts start",
    "build": "react-scripts build && node license-build.js",
    "test": "react-scripts test --env=jsdom --resetMocks=false --clearMocks=true",
    "test:ci": "CI=true react-scripts test --env=jsdom --resetMocks=false --ci --testTimeout 7000 --clearMocks=true",
    "eject": "react-scripts eject",
    "ts-check": "tsc && tsc --noEmit --project e2e/tsconfig.json",
    "eslint": "eslint src/ e2e/",
    "lint": "yarn ts-check && yarn eslint",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "test:e2e": "ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 testcafe chrome e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:e2e:ci": "PORT=8080 testcafe chrome:headless e2e/tests/*.ts --compiler-options typescript.configPath='e2e/tsconfig.json'",
    "test:browserstack": "ZEEBE_GATEWAY_ADDRESS=localhost:26503 PORT=3001 node e2e/scripts/browserstack.js",
    "test:ci-browserstack": "PORT=8080 node e2e/scripts/browserstack.js",
    "postinstall": "cd .. && husky install",
    "coverage": "yarn test --coverage --collectCoverageFrom=src/**/* --changedSince=origin/master --watchAll=false --passWithNoTests",
    "analyze-bundle": "yarn build && source-map-explorer 'build/static/js/*.js'"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@camunda-cloud/common-ui.*)"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/testcafe": "4.4.1",
    "@testing-library/user-event": "14.4.3",
    "@types/carbon-components-react": "7.55.2",
    "@types/carbon__elements": "10.35.0",
    "@types/jest": "27.5.2",
    "@types/lodash": "4.14.183",
    "@types/mixpanel-browser": "2.38.0",
    "@types/node": "16.11.49",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-transition-group": "4.4.5",
    "@types/styled-components": "5.1.26",
    "eslint-plugin-camunda-licensed": "0.4.5",
    "eslint-plugin-prettier": "4.2.1",
    "glob": "8.0.3",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.1",
    "jest-styled-components": "7.1.0",
    "lint-staged": "13.0.3",
    "monaco-editor": "0.34.0",
    "msw": "0.42.1",
    "node-fetch": "2.6.7",
    "prettier": "2.7.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "source-map-explorer": "2.5.2",
    "testcafe": "1.20.1",
    "testcafe-browser-provider-browserstack": "1.14.0",
    "ts-toolbelt": "9.6.0",
    "typescript": "4.7.4",
    "zeebe-node": "8.0.1"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scarfSettings": {
    "enabled": false
  },
  "msw": {
    "workerDirectory": "public"
  },
  "resolutions": {
    "react-scripts/postcss-preset-env/postcss-custom-properties": "^10.0.0"
  }
}
