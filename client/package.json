{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "array-includes": "^3.0.3",
    "array.prototype.find": "^2.0.4",
    "bpmn-js": "0.23.2",
    "chart.js": "^2.7.1",
    "element-closest": "^2.0.2",
    "heatmap.js": "^2.0.5",
    "immutability-helper": "^2.6.0",
    "moment": "^2.19.1",
    "object.values": "^1.0.4",
    "react": "16.2.0",
    "react-date-range": "^0.9.4",
    "react-dom": "16.2.0",
    "react-full-screen": "^0.2.2",
    "react-router-dom": "^4.2.2",
    "string.prototype.includes": "^1.0.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./src/shared-styles src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/shared-styles src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "node scripts/wireModules",
    "start-backend": "node scripts/start-backend",
    "dependencies": "node scripts/dependencies.js"
  },
  "lint-staged": {
    "src/**/*.{js,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "camunda-bpm-sdk-js": "camunda/camunda-bpm-sdk-js",
    "chalk": "^2.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest-enzyme": "^4.0.1",
    "js-yaml": "^3.10.0",
    "license-checker": "^14.0.0",
    "lint-staged": "^7.0.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "1.11.1",
    "react-router": "^4.2.0",
    "react-scripts": "1.0.14",
    "request": "^2.83.0",
    "shelljs": "^0.7.8",
    "svg-react-loader": "^0.4.5",
    "tarball-extract": "^0.0.6"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8090"
    }
  }
}
