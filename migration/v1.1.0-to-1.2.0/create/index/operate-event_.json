{
    "aliases" : {
      "operate-event_alias" : {
        "is_write_index" : true
      }
    },
    "mappings" : {
      "_doc" : {
        "dynamic" : "strict",
        "dynamic_templates" : [
          {
            "string_template" : {
              "path_match" : "*",
              "match_mapping_type" : "string",
              "mapping" : {
                "index_options" : "docs",
                "norms" : {
                  "enabled" : false
                },
                "type" : "string"
              }
            }
          }
        ],
        "properties" : {
          "activityId" : {
            "type" : "keyword"
          },
          "bpmnProcessId" : {
            "type" : "keyword"
          },
          "dateTime" : {
            "type" : "date",
            "format" : "date_time"
          },
          "eventSourceType" : {
            "type" : "keyword"
          },
          "eventType" : {
            "type" : "keyword"
          },
          "flowNodeInstanceKey" : {
            "type" : "long"
          },
          "id" : {
            "type" : "keyword"
          },
          "key" : {
            "type" : "long"
          },
          "metadata" : {
            "properties" : {
              "incidentErrorMessage" : {
                "type" : "keyword"
              },
              "incidentErrorType" : {
                "type" : "keyword"
              },
              "jobCustomHeaders" : {
                "type" : "object",
                "enabled" : false
              },
              "jobDeadline" : {
                "type" : "date",
                "format" : "date_time"
              },
              "jobKey" : {
                "type" : "long"
              },
              "jobRetries" : {
                "type" : "integer"
              },
              "jobType" : {
                "type" : "keyword"
              },
              "jobWorker" : {
                "type" : "keyword"
              }
            }
          },
          "partitionId" : {
            "type" : "integer"
          },
          "payload" : {
            "type" : "keyword"
          },
          "workflowInstanceKey" : {
            "type" : "long"
          },
          "workflowKey" : {
            "type" : "long"
          }
        }
      }
    },
    "settings" : {
      "index" : {
        "refresh_interval" : "2s",
        "number_of_shards" : "1",
        "analysis" : {
          "normalizer" : {
            "case_insensitive" : {
              "filter" : "lowercase"
            }
          }
        },
        "number_of_replicas" : "0"
      }
    }
  }