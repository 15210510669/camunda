            "type" : "long"
          },
          "workflowKey" : {
            "type" : "long"
          },
          "workflowName" : {
            "type" : "keyword",
            "normalizer" : "case_insensitive"
          },
          "workflowVersion" : {
            "type" : "long"
          }
        }
      }
    },
    "settings" : {
