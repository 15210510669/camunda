{
  "mappings": {
    "single-process-report": {
      "dynamic": "strict",
      "dynamic_templates": [
        {
          "string_template": {
            "path_match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "index_options": "docs",
              "norms": {
                "enabled": false
              },
              "type": "string"
            }
          }
        }
      ],
      "properties": {
        "combined": {
          "type": "boolean"
        },
        "created": {
          "type": "date",
          "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
        },
        "data": {
          "type": "object",
          "enabled": false
        },
        "id": {
          "type": "keyword"
        },
        "lastModified": {
          "type": "date",
          "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
        },
        "lastModifier": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "owner": {
          "type": "keyword"
        },
        "reportType": {
          "type": "keyword"
        }
      }
    }
  }
}