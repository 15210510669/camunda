{
  "index_patterns": [
    "zeebe-record-job-batch_*"
  ],
  "order": 20,
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0
  },
  "mappings": {
    "_doc": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "type": {
              "type": "keyword"
            },
            "worker": {
              "type": "keyword"
            },
            "timeout": {
              "type": "date"
            },
            "amount": {
              "type": "integer"
            },
            "jobKeys": {
              "type": "long"
            },
            "jobs": {
              "type": "nested",
              "dynamic": "strict",
              "properties": {
                "type": {
                  "type": "keyword"
                },
                "headers": {
                  "properties": {
                    "activityId": {
                      "type": "keyword"
                    },
                    "activityInstanceKey": {
                      "type": "long"
                    },
                    "bpmnProcessId": {
                      "type": "keyword"
                    },
                    "workflowDefinitionVersion": {
                      "type": "integer"
                    },
                    "workflowInstanceKey": {
                      "type": "long"
                    },
                    "workflowKey": {
                      "type": "long"
                    }
                  }
                },
                "customHeaders": {
                  "dynamic": true,
                  "type": "object"
                },
                "worker": {
                  "type": "keyword"
                },
                "retries": {
                  "type": "long"
                },
                "deadline": {
                  "type": "date"
                },
                "payload": {
                  "type": "text"
                }
              }
            }
          }
        }
      }
    }
  }
}
