                },
                "duplicate": {
                  "type": "boolean"
                }
              }
            },
            "resources": {
              "properties": {
                "resource": {
                  "enabled": false
                },
                "resourceName": {
                  "type": "text"
                }
              }
            },
            "decisionRequirementsMetadata": {
              "properties": {
                "tenantId": {
                  "type": "keyword"
                },
                "decisionRequirementsId": {
                  "type": "keyword"
                },
