{
  "index_patterns": [
    "zeebe-record_deployment_*"
  ],
  "priority": 20,
  "version": 1,
  "template": {
    "aliases": {
      "zeebe-record-deployment": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "deployedProcesses": {
              "properties": {
                "bpmnProcessId": {
                  "type": "keyword"
                },
                "version": {
                  "type": "long"
                },
                "processDefinitionKey": {
                  "type": "long"
                },
                "resourceName": {
                  "type": "text"
                },
                "resource": {
                  "enabled": false
                },
                "checksum": {
                  "enabled": false
                }
              }
            },
            "resources": {
              "properties": {
                "resource": {
                  "enabled": false
                },
                "resourceName": {
                  "type": "text"
                }
              }
            }
          }
        }
      }
    }
  }
}
