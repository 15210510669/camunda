{
  "index_patterns": [
    "zeebe-record_message_*"
  ],
  "priority": 20,
  "version": 1,
  "template": {
    "aliases": {
      "zeebe-record-message": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "name": {
              "type": "keyword"
            },
            "correlationKey": {
              "type": "text"
            },
            "messageId": {
              "type": "keyword"
            },
            "timeToLive": {
              "type": "long"
            },
            "variables": {
              "enabled": false
            },
            "deadline": {
              "type": "long"
            }
          }
        }
      }
    }
  }
}
