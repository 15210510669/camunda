# https://github.com/kubernetes-sigs/kustomize/blob/master/cmd/config/docs/tutorials/configuration-basics.md
apiVersion: config.k8s.io/v1alpha1
kind: Krmfile
openAPI:
  definitions:
    io.k8s.cli.setters.created-by:
      description: used to set camunda.cloud/created-by
      type: string
      x-k8s-cli:
        setter:
          name: created-by
    io.k8s.cli.setters.managed-by:
      description: used to set camunda.cloud/managed-by
      type: string
      x-k8s-cli:
        setter:
          name: managed-by
    io.k8s.cli.setters.source:
      description: used to set camunda.cloud/source
      type: string
      x-k8s-cli:
        setter:
          name: source
